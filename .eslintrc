{
    "rules": {
        "quotes": [
            2,
            "single",
            { "allowTemplateLiterals": true }
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": 0,
        "no-unused-vars": 0,
        "curly": 0,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    parserOptions: {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
    },
    "globals": {
    }
}